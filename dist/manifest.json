{
  "name": "Simple Background Sounds",
  "version": "1.0.0",
  "description": "Run background sounds",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["js/background.js"]
   },
  "browser_action": {
    "default_popup": "popup.html"
  }
}